<br><br><br>

<h4>Categories</h4>

<table class="striped">
  <thead>
    <tr>
      <th data-field="id">Id</th>
      <th data-field="name">Name</th>
    </tr>
  </thead>
  <tbody>
    <%= for category <- @categories do %>
      <tr>
          <td> <%= category.id %> </td>
          <td> <%= category.name %> </td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link "new", to: category_path(@conn, :new), class: "btn btn-primary" %>
